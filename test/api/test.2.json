{
  "name": "Test httpbin/get",
  "tests": [
    {
      "name": "Returns with args `a: b`",
      "request": {
        "url": "https://httpbin.org/get?a=b"
      },
      "response": {
        "content-type": "application/json",
        "status-code": {
          "is-at-least": 200,
          "is-at-most": 201
        },
        "timing.dns-resolution": {
          "is-below": 100
        },
        "timing.total": {
          "is-below": 500
        },
        "body.args.a": "b"
      }
    }, {
      "name": "Something else",
      "request": {
        "url": "https://httpbin.org/get?a=b"
      },
      "response": {
        "content-type": "application/json",
        "status-code": {
          "equal": 200
        },
        "timing.dns-resolution": {
          "is-below": 100
        },
        "timing.total": {
          "is-below": 500
        }
      }
    }
  ]
}